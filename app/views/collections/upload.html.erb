<div class="container">  
  <div class="row">
    <div class="col-sm-6 col-md-4 col-md-offset-4">
      <h1 class="text-center login-title">Import Data</h1><br><br>
      <div class="account-wall">
          <%= form_tag import_collections_path, multipart: true do %>
            <div>
              <%= select_tag :category, options_from_collection_for_select(@categories, :name, :name) , class: "form-control" %> <br>
            </div>
            <%= file_field_tag :file %> <br>
            <%= submit_tag "Import" ,class: "btn btn-primary" %>
          <% end %>
			</div>
		</div>
	</div>
</div>